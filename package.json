    "cheerio": "^1.0.0-rc.12",
    "fast-xml-parser": "^4.5.1",
    "franc": "^6.2.0",
    "groq-sdk": "^0.37.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "next": "14.2.13",
    "next-intl": "^4.5.5",
    "next-seo": "^6.6.0",
    "node-cron": "^3.0.3",
