{
  "name": "mehmetkucuk-news-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build && npm run bots:build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "bots:scrape": "tsx src/bots/scheduler.ts",
    "bots:runOnce": "tsx src/bots/scheduler.ts --runOnce",
    "bots:build": "tsc -p tsconfig.bots.json && tsc-alias -p tsconfig.bots.json",
    "metrics:dev": "tsx src/api/metrics/server.ts"
  },
